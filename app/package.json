    "express-http-proxy": "^2.1.1",
    "maplibre-gl": "^4.7.1"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/express-http-proxy": "^1.6.6",
    "@types/glob": "^8.1.0",
    "concurrently": "^9.1.0",
    "esbuild": "^0.24.0",
    "glob": "^11.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
